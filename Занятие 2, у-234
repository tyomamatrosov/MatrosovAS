from math import*
x1 = 14.26
y1 = -1.22
z1 = 3.5*10**-2

x2 = -4.5
y2 = 0.75*10**-4
z2 = -0.845*10**2

x13 = 17.421
y13 = 10.365*10**-3
z13 = 0.828*10**5

s1 =  ((2*cos(x1-2/3)) / (1/2 + sin(y1)**2)) * (1+ ((z1**2) / (3 - z1**2/5)))
s2 = (((9+((x2-y2)**2)) ** (1./3.)) / (x2**2 + y2**2 + 2)) - (exp(abs(x2-y2)) * tan(z2)**3)
s13 = ((y13+((x13-1)**(1./3.)))**(1./4.)) / (abs(x13-y13) * (sin(z13)**2 + tan(z13)))

print(s1, s2, s13)
